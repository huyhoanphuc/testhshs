	if newHealth == 0 then
		print("Entity has killed the player")
	else
	game.workspace.Asitty.RushNew.Anchored = false
	workspace.Asitty.RushNew.Scream:Stop()
    workspace.Asitty.RushNew.FarStatic:Stop()
    workspace.Asitty.RushNew.FarScream:Stop()
wait(2)

