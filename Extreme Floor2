function Smile()
pcall(function()
while true do wait(160)
local roomValue = game.ReplicatedStorage.GameData.LatestRoom.Value
if not ((roomValue >= 18 and roomValue <= 24) or (roomValue >= 30 and roomValue <= 35)  or (roomValue >= 40 and roomValue <= 53) or (roomValue >= 93 and roomValue <= 321)) then
wait()
game.ReplicatedStorage.GameData.LatestRoom.Changed:Wait()
game.Lighting.MainColorCorrection.TintColor = Color3.fromRGB(255, 0, 0)
game.Lighting.MainColorCorrection.Contrast = 0.3
local tween = game:GetService("TweenService")
tween:Create(game.Lighting.MainColorCorrection, TweenInfo.new(0.5), {Contrast = 0}):Play()
local TweenService = game:GetService("TweenService")
local TW = TweenService:Create(game.Lighting.MainColorCorrection, TweenInfo.new(0.5),{TintColor = Color3.fromRGB(255, 255, 255)})
TW:Play()
local Spawner = loadstring(game:HttpGet("https://raw.githubusercontent.com/RegularVynixu/Utilities/main/Doors/Entity%20Spawner/V1/Source.lua"))()

---====== Create entity ======---

local entity = Spawner.createEntity({
    CustomName = "Smile",
    Model = "rbxassetid://9690404405", -- Your entity's model url here ("rbxassetid://1234567890" or GitHub raw url)
    Speed = 670,
    MoveDelay = 0.8,
    HeightOffset = 0,
    CanKill = true,
    KillRange = 40,
    SpawnInFront = false,
    ShatterLights = false,
    FlickerLights = {
        Enabled = true,
        Duration = 8
    },
    Cycles = {
        Min = 1,
        Max = 1,
        Delay = 0
    },
    CamShake = {
        Enabled = true,
        Values = {5.15, 20, 0.1, 1},
        Range = 100
    },
    ResistCrucifix = false,
    BreakCrucifix = false,
    DeathMessage = {"you die to A-10", "yee."},
    IsCuriousLight = true
})

---====== Debug ======---

