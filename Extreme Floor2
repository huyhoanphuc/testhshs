						 local Spawner = loadstring(game:HttpGet("https://raw.githubusercontent.com/RegularVynixu/Utilities/main/Doors/Entity%20Spawner/V1/Source.lua"))()

---====== Create entity ======---

local entity = Spawner.createEntity({
    CustomName = "Monster",
    Model = "rbxassetid://11470738519", -- Your entity's model url here ("rbxassetid://1234567890" or GitHub raw url)
    Speed = 500,
    MoveDelay = 10,
    HeightOffset = 0,
    CanKill = true,
    KillRange = 1000000000,
    SpawnInFront = false,
    ShatterLights = true,
    FlickerLights = {
        Enabled = false,
        Duration = 8
    },
    Cycles = {
        Min = 1.5,
        Max = 1.5,
        Delay = 0
    },
    CamShake = {
        Enabled = true,
        Values = {3.5, 20, 0.1, 1},
        Range = 600
    },
    ResistCrucifix = false,
    BreakCrucifix = false,
    DeathMessage = {"you die to A-10", "yee."},
    IsCuriousLight = true
})

