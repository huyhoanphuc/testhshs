coroutine.wrap(function()
function GitAud(soundgit,filename)
    SoundName=tostring(SoundName)
    local url=soundgit
    local FileName = filename
    writefile(FileName..".mp3", game:HttpGet(url))
    return (getcustomasset or getsynasset)(FileName..".mp3")
end

function CustomGitSound(soundlink, vol, filename)
    local Sound = Instance.new("Sound")
    Sound.SoundId = GitAud(soundlink, filename)
    Sound.Parent = workspace
    Sound.Volume = 0
    Sound:Play()
end

CustomGitSound("https://github.com/huyhoanphuc/hgss/blob/main/km_20240919_720p_12f_20240919_022413.mp3?raw=true", 1, "Hereicoahgskhmeseekog")

local function GetGitSoundID(GithubSnd,SoundName)
		SoundName=tostring(SoundName)
		local url=GithubSnd
		SoundName=tostring(SoundName)
		local url=GithubSnd
		FileName = SoundName
		writefile("customObject_Sound_"..FileName..".mp3", game:HttpGet(url))
		return (getcustomasset or getsynasset)("customObject_Sound_"..FileName..".mp3")
	end
end)()

