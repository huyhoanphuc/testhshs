local Dumy = game:GetObjects("rbxassetid://96223289630255")[1]
local Matchers = game:GetObjects("rbxassetid://111920803011003")[1]
Dumy.Parent = workspace
Matchers.Parent = workspace

if Workspace.Frostbite then
    game.Workspace.Insidea:Destroy()
  end

local primary_part = Matchers:FindFirstChildWhichIsA("BasePart") or Matchers:FindFirstChildWhichIsA("Part")

game.workspace.insidea.Main.Anchored = true

Dumy.HumanoidRootPart.CFrame = Character.HumanoidRootPart.CFrame + Vector3.new(0, 0, -126)

local function followPlayer()
    local Direction = (Character.HumanoidRootPart.Position - Dumy.HumanoidRootPart.Position).unit
    Dumy.HumanoidRootPart.Velocity = Direction * Speed
    Dumy.HumanoidRootPart.CFrame = CFrame.new(Dumy.HumanoidRootPart.Position, Character.HumanoidRootPart.Position)
end

RunService.RenderStepped:Connect(function()
if Matchers:IsDescendantOf(workspace) and Dumy:IsDescendantOf(workspace) and Dumy:FindFirstChild("HumanoidRootPart") and Dumy:FindFirstChild("Humanoid").Health > 0 then
local Distance = (Character.HumanoidRootPart.Position - Dumy.HumanoidRootPart.Position).magnitude

if Distance <= math.huge then
followPlayer()
end

Matchers:PivotTo(Dumy.HumanoidRootPart.CFrame)
end
end)

wait(10)
game.workspace.insidea.Main.CanCollide = true
wait(0.1)
