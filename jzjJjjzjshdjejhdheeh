---====== Debug entity ======---

entity:SetCallback("OnSpawned", function()
    print("Entity has spawned")
    if Workspace.Frostbite then
    game.Workspace.Frostbite:Destroy()
    end
    
    wait(5)
local Speed = 12

local Workspace = game:GetService("Workspace")
local Players = game:GetService("Players")
local RunService = game:GetService("RunService")
local Debris = game:GetService("Debris")
local Player = Players.LocalPlayer
local Character = Player.Character or Player.CharacterAdded:Wait()

local Dumy = game:GetObjects("rbxassetid://96223289630255")[1]
local Matchers = game:GetObjects("rbxassetid://111920803011003")[1]
Dumy.Parent = workspace
Matchers.Parent = workspace

if Workspace.Frostbite then
    game.Workspace.Insidea:Destroy()
  end

local primary_part = Matchers:FindFirstChildWhichIsA("BasePart") or Matchers:FindFirstChildWhichIsA("Part")

game.workspace.insidea.Main.Anchored = true

Dumy.HumanoidRootPart.CFrame = Character.HumanoidRootPart.CFrame + Vector3.new(0, 0, -126)

local function followPlayer()
    local Direction = (Character.HumanoidRootPart.Position - Dumy.HumanoidRootPart.Position).unit
    Dumy.HumanoidRootPart.Velocity = Direction * Speed
    Dumy.HumanoidRootPart.CFrame = CFrame.new(Dumy.HumanoidRootPart.Position, Character.HumanoidRootPart.Position)
end

RunService.RenderStepped:Connect(function()
if Matchers:IsDescendantOf(workspace) and Dumy:IsDescendantOf(workspace) and Dumy:FindFirstChild("HumanoidRootPart") and Dumy:FindFirstChild("Humanoid").Health > 0 then
local Distance = (Character.HumanoidRootPart.Position - Dumy.HumanoidRootPart.Position).magnitude

if Distance <= math.huge then
followPlayer()
end

Matchers:PivotTo(Dumy.HumanoidRootPart.CFrame)
end
end)

wait(10)
game.workspace.insidea.Main.CanCollide = true
wait(0.1)
if not game.Players.LocalPlayer.Character:GetAttribute("Hiding") then

wait()
function GitAud(soundgit,filename)
    SoundName=tostring(SoundName)
    local url=soundgit
    local FileName = filename
    writefile(FileName..".mp3", game:HttpGet(url))
    return (getcustomasset or getsynasset)(FileName..".mp3")
end

function CustomGitSound(soundlink, vol, filename)
    local Sound = Instance.new("Sound")
    Sound.SoundId = GitAud(soundlink, filename)
    Sound.Parent = workspace
    Sound.Volume = 3
    Sound:Play()
end

CustomGitSound("https://github.com/huyhoanphuc/testhshs/blob/main/km_20241017-1_1080p_15f_20241017_183631.mp3?raw=true", 1, "acareOmg")

function GitPNG(GithubImg, ImageName)
    local url = GithubImg
    if not isfile(ImageName .. ".png") then
        local success, response = pcall(function()
            return game:HttpGet(url)
        end)

        if success then
            writefile(ImageName .. ".png", response)
        else
            error(": " .. response)
        end
    end
    return (getcustomasset or getsynasset)(ImageName .. ".png")
end

game.Players.LocalPlayer.Character.Humanoid.Health = 0
game.Players.LocalPlayer.Character:FindFirstChildWhichIsA("Humanoid"):TakeDamage(10000000000000)

    local JumpscareGui = Instance.new("ScreenGui")
    local Background = Instance.new("Frame")
    local Face = Instance.new("ImageLabel")

    JumpscareGui.Name = "JumpscareGui"
    JumpscareGui.IgnoreGuiInset = true
    JumpscareGui.ZIndexBehavior = Enum.ZIndexBehavior.Sibling
    JumpscareGui.Parent = game.Players.LocalPlayer:WaitForChild("PlayerGui")

    Background.Name = "Background"
    Background.BackgroundColor3 = Color3.fromRGB(255, 0, 0)
    Background.BorderSizePixel = 0
    Background.Size = UDim2.new(1, 0, 1, 0)
    Background.ZIndex = 999
    Background.BackgroundTransparency = 0.6

    Face.Name = "Face"
    Face.AnchorPoint = Vector2.new(0.5, 0.5)
    Face.BackgroundTransparency = 1
    Face.Position = UDim2.new(0.5, 0, 0.5, 0)
    Face.ResampleMode = Enum.ResamplerMode.Pixelated
    Face.Size = UDim2.new(0, 0, 0, 0)
    Face.Image = GitPNG("https://github.com/huyhoanphuc/testhshs/blob/main/cropped-%D7%91%D7%90%D7%A0%D7%A8-%D7%A8%D7%A7%D7%A2-%D7%A9%D7%A7%D7%95%D7%A3-2-2%20(1).png?raw=true", "A6nSteroidfssss")
    Face.ImageColor3 = Color3.fromRGB(255, 255, 255)
    Face.ImageTransparency = 0

    Face.Parent = Background
    Background.Parent = JumpscareGui

    game.TweenService:Create(Face, TweenInfo.new(1), {Size = UDim2.new(0, 850, 0, 850), ImageTransparency = 0}):Play()
    game.TweenService:Create(Face, TweenInfo.new(1), {Rotation = math.random(-10,10)}):Play()
    task.wait(0.2)
    game.TweenService:Create(Face, TweenInfo.new(0.6), {ImageTransparency = 1}):Play()
game.TweenService:Create(Background, TweenInfo.new(10), {BackgroundTransparency = 1}):Play()
    task.wait(8)
    JumpscareGui:Destroy()
end

game.workspace.insidea:Destroy()
